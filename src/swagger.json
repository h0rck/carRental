{
  "openapi":"3.0.0",
  "info": {
    "title": "Alguel de carros",
    "description": "Essa API faz o aluguel de carros",
    "termsOfService":"http:localhost:3000",
    "contact": {
      "email":"samukarp@outlook.com"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url":"http:localhost:3000",
      "description":"DEV"
    }
  ],
  "paths": {
    "/user/register":{
      "post":{
        "summary": "Cadastro de produto",
        "description": "Essa rota será responsável por cadastrar um novo produto",
        "tags":["Usuarios"],
        "requestBody":{
          "content":{
            "applicaation/json":{
              "schema":{
                  "$ref": "#/components/schemas/Usuarios"
              },
              "examples":{
                "Usuarios": {
                  "value": {
                    "name": "Samuel Ribeiro",
                    "email":"samukarp@outlook.com",
                    "senha": "asd123"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "401":{
            "description": "Não autorizado"
          },
          "400":{
            "description": ""
          },
          "200":{
            "description":"OK",
            "content": {
              "applicaation/json":{
                "schema":{
                  "items":{
                    "type":"object",
                    "user":{
                      "name":{
                        "type":"string"
                      },
                      "email":{
                        "type": "string"
                      }
                    },
                    "token":{
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components":{
    "schemas":{
      "Usuarios":{
        "type":"object",
        "name":{
          "type":"string"
        },
        "email":{
          "type": "string"
        },
        "senha": {
          "type":"string"
        }
      }
    }
  }
}
